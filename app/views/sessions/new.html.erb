<h1 align='center'> Login </h1>

<%= form_for :session, url: login_path do |f|%>
  <p>
    <%= f.label :email %><br>
    <%= f.email_field :email%>
  </p>
    <%= f.label :password %><br>
    <%= f.password_field :password, autocomplete: 'off' %>
  </p>
  <p>
  <%= f.submit "login" %>
  </p>
<%end%>